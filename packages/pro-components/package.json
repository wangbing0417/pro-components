{
  "name": "pro-components",
  "version": "0.0.1",
  "main": "dist/pro-ui.cjs.js",
  "module": "dist/pro-ui.esm-bundler.js",
  "unpkg": "dist/pro-ui.global.js",
  "jsdelivr": "dist/pro-ui.global.js",
  "files": [
    "lib",
    "dist",
    "src",
    "types"
  ],
  "typings": "types/index.d.ts",
  "scripts": {
    "build": "rollup -c",
    "dev": "concurrently \"npm run generateCssFile\" \"npm run gulp\" \"npm run rollup\" \"npm run gulp:build\"",
    "rollup": "cross-env NODE_ENV=development rollup -wc",
    "generateCssFile": "cd scripts && nodemon generateCssFile.js",
    "gulp": "gulp watch",
    "gulp:build": "gulp build"
  },
  "license": "MIT",
  "style": "lib/theme-chalk/index.css",
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.12.1",
    "@babel/plugin-proposal-optional-chaining": "^7.12.7",
    "@babel/preset-env": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-replace": "^2.3.4",
    "@vue/babel-plugin-jsx": "^1.0.0-rc.4",
    "@vue/compiler-sfc": "^3.0.4",
    "concurrently": "^5.3.0",
    "cp-cli": "^2.0.0",
    "gulp": "^4.0.0",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-cssmin": "^0.2.0",
    "gulp-sass": "^4.1.0",
    "nodemon": "^2.0.7",
    "rollup": "^2.28.2",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-scss": "^2.6.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.29.0",
    "rollup-plugin-vue": "^6.0.0",
    "typescript": "^4.1.2",
    "vue": "^3.0.4",
    "vue-router": "^4.0.1"
  }
}
